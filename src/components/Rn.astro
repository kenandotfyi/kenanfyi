<script>
  //@ts-nocheck
  import { annotate } from "rough-notation";

  const e = document.querySelectorAll("#rn").forEach((hl) => {
    const annotation = annotate(hl, {
      type: "bracket",
      color: "red",
      padding: [0, 10],
      brackets: ["left", "right", "top", "bottom"],
      strokeWidth: 3,
    });
    setTimeout(() => {
      annotation.show();
    }, 1000);
  });
</script>
