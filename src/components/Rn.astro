<script>
  //@ts-nocheck
  import { annotate } from "rough-notation";

  const e = document.querySelectorAll("#rn").forEach((highlight) => {
    const annotation = annotate(highlight, {
      type: "highlight",
      color: "var(--orange)",
      padding: [0, 10],
      brackets: ["left", "right", "top", "bottom"],
      strokeWidth: 3,
    });
    setTimeout(() => {
      annotation.show();
    }, 500);
  });
</script>
